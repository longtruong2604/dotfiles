[init]  
	defaultBranch = main
[user]
	email = zachhung911@gmail.com
	name = ZachHung
[alias]
	hist = log --pretty=format:'%C(yellow)[%ad]%C(reset) %C(green)[%h]%C(reset) | %C(white)%s %C(bold cyan){{%an}}%C(reset) %C(blue)%d%C(reset)' --graph --date=short
	co = checkout
	cob = checkout -b
	coo = !git fetch && git checkout
	sw = switch 
	swc = switch -c
	br = branch
	brd = branch -d
	st = status
	aa = add -A .
	unstage = reset --soft HEAD^
	cm = commit -m
	amend = commit --amend -m
	fix = commit --fixup
	undo = reset HEAD~1
	rv = revert
	cp = cherry-pick
	pu = !git push origin `git branch --show-current`
	fush = push -f
	mg = merge --no-ff
	rb = rebase
	rbc = rebase --continue
	rba = rebase --abort
	rbs = rebase --skip
	rom = !git fetch && git rebase -i origin/master --autosquash
	save = stash push
	pop = stash pop
	apply = stash apply
	rl = reflog
